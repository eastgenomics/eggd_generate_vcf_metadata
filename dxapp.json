{
    "name": "eggd_generate_vcf_metadata",
    "title": "eggd_generate_vcf_metadata",
    "summary": "Generate metadata yaml configs for given VCF file to upload to openCGA",
    "dxapi": "1.0.0",
    "version": "1.0.0",
    "githubRelease": "v1.0.0",
    "inputSpec": [
      {
        "name": "vcf",
        "label": "vcf",
        "class": "array:file",
        "optional": false,
        "patterns": [
          "*"
        ],
        "help": ""
      }
    ],
    "outputSpec": [
      {
        "name": "config_zip",
        "label": "config zip",
        "class": "file",
        "patterns": [
          "*.zip"
        ],
        "help": "zip of required config files for opencga upload"
      }
    ],
    "runSpec": {
      "timeoutPolicy": {
        "*": {
          "hours": 1
        }
      },
      "interpreter": "bash",
      "file": "src/eggd_generate_vcf_metadata.sh",
      "distribution": "Ubuntu",
      "release": "20.04",
      "version": "0"
    },
    "access": {
      "project": "CONTRIBUTE",
      "allProjects": "VIEW"
    },
    "authorizedUsers": [
      "org-emee_1"
    ],
    "developers":[
      "org-emee_1"
    ],
    "regionalOptions": {
      "aws:eu-central-1": {
        "systemRequirements": {
          "*": {
            "instanceType": "mem1_ssd1_v2_x2"
          }
        }
      }
    }
  }
